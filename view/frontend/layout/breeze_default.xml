<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="dynamic" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Amasty_Base/vendor/slick/slick.min" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Base/vendor/slick/slick.min</item>
                            </item>
                            <item name="Amasty_Promo/js/discount-calculator" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Promo/js/discount-calculator</item>
                            </item>
                            <item name="Amasty_Promo/js/popup" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_Promo/js/popup</item>
                                <item name="import" xsi:type="array">
                                    <item name="3" xsi:type="string">Amasty_Promo/js/discount-calculator</item>
                                    <item name="5" xsi:type="string">Amasty_Base/vendor/slick/slick.min</item>
                                </item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
